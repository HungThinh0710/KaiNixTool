{
  "name": "KaiNix",
  "version": "0.1.0",
  "description": "This is an open source tool for testing REST API, power on Electronjs.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=build",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=build --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"KaiNix Tool\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=build"
  },
  "repository": "https://github.com/HungThinh0710/KaiNixTool",
  "keywords": [
    "Electron",
    "Kainix",
    "api",
    "testing",
    "sict"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dateformat": "^3.0.3",
    "electron-store": "^5.1.1"
  }
}
